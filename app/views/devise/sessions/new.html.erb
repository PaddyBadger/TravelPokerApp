<div class="form">
  <%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
    <div class="topDeck">
      <h3>Sign in</h3>
    </div>

    <%= f.label :email, class: 'formlabel' %>
    <div class="tinput">
      <%= f.email_field :email%>
    </div>

    <%= f.label :password, class: 'formlabel' %>
    <div class="tinput">
      <%= f.password_field :password %>
    </div>

    <% if devise_mapping.rememberable? -%>
    <div class="formLinks">
      <div><%= f.check_box :remember_me %> <%= f.label :remember_me %></div>
    </div>
    <% end -%>

     <div class="cbutton">
      <%= f.submit "Sign In", class: 'btn btn-large' %>
    </div>
  <% end %>
  <div class="formLinks">
    <%= render "devise/shared/links" %>
  </div>

  <div class="seasons">
    <div class="jan">
      <h1>Jan</h1>
    </div>
    <div class="feb">
      <h1>Feb</h1>
    </div>
    <div class="mar">
      <h1>Mar</h1>
    </div>
    <div class="apr">
      <h1>Apr</h1>
    </div>
    <div class="may">
      <h1>May</h1>
    </div>
    <div class="jun">
      <h1>Jun</h1>
    </div>
    <div class="jul">
      <h1>Jul</h1>
    </div>
    <div class="aug">
      <h1>Aug</h1>
    </div>
    <div class="sep">
      <h1>Sep</h1>
    </div>
    <div class="oct">
      <h1>Oct</h1>
    </div>
    <div class="nov">
      <h1>Nov</h1>
    </div>
    <div class="dec">
      <h1>Dec</h1>
    </div>
  </div>
</div>