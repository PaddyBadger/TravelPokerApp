<div class="topDeck">
  <h3><%= @deck.title %></h3>
</div>
  <div class="tripBack">
    <%= gmaps4rails(@json) %>
    <%= image_tag @deck.image%>
    <div class="tripInfoBack">
      <h3><%= @deck.title%></h3>
    </div>
  </div>
<div class="deck_back">
  <div class="likes1"><%= render partial: 'likes/like', locals: { likeable: @deck } %></div>
  <div class="dk_container span3 dk_shown dk_theme_default" id="dk_container_herolist" tabindex="1">
    <div class="dk_toggle">
     <span class="dk_label" id="keep1">Keep</span>
    </div>
    <div class="dk_options_1">
      <ul class="dk_options_inner">
      <% if current_user %>
        <li class="">
          <%= link_to "Keep Deck", method: :post, remote: true %>
        </li>
      <% else %>
        <li><a href="/decks/new/" data-dk-dropdown-value="1">Log In | Register?</a></li>
      <% end %>
        <li><div class="likes1"><%= render partial: 'likes/like', locals: { likeable: @deck } %></div></li>
      </ul>
    </div>
  </div>
  <div class="dk_container span3 dk_shown dk_theme_default" id="dk_container_herolist" tabindex="1">
    <div class="dk_toggle">
      <span class="dk_label" id="keep1">Share</span>
    </div>
    <div class="dk_options_1">
      <ul class="dk_options_inner">
        <li class="social_1">
          <a href="#"><img src="/assets/twitter.png"></a>
          <a href="#"><img src="/assets/facebook.png"></a>
          <a href="#"><img src="/assets/pinterest.png"></a>
          <a href="#"><img src="/assets/rss.png"></a>
        </li>
      </ul>
    </div>
  </div>
</div>
<div class="deckCards">
  <div class="topDeck">
    <%= render @cards %>
    <div class="apple_pagination">
      <%= will_paginate @cards %>
    </div>
  </div>
  <% if can? :manage, Deck %>
    <div class="memberOnlyDeck">
      <span><%= link_to "Edit", edit_deck_path(@deck), class: "keep11", id: "keep1" %></span>
      <span> <%= link_to "Delete", @deck, method: :delete, class: 'keep11', id: "keep1", confirm: 'Are you sure you want to delete this deck?' %></span>
    </div>
  <% end %>
</div>



