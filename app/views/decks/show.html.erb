<div class="topDeck">
  <h3><%= @deck.title %></h3>
</div>
  <div class="tripBack">
    <%= gmaps4rails(@json) %>
    <%= image_tag @deck.image%>
    <div class="tripInfoBack">
      <h3><%= @deck.title%></h3>
    </div>
  </div>
<div class="deck_back">
  <div class="likes1"><%= render partial: 'likes/like', locals: { likeable: @deck } %></div>
  <%= render partial: 'decks/keep', locals: { deck: @deck } %>
  <%= render partial: 'decks/social', locals: { deck: @deck } %>
</div>
<div class="deckCards">
  <div class="topDeck">
    <%= render @cards %>
    <div class="apple_pagination">
      <%= will_paginate @cards %>
    </div>
  </div>
  <% if can? :manage, Deck %>
    <div class="memberOnlyDeck">
      <span><%= link_to "Edit", edit_deck_path(@deck), class: "keep11", id: "keep1" %></span>
      <span> <%= link_to "Delete", @deck, method: :delete, class: 'keep11', id: "keep1", confirm: 'Are you sure you want to delete this deck?' %></span>
    </div>
  <% end %>
</div>



