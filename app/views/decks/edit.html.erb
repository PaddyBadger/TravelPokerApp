<div class="topDeck">
  <h3>Edit <%= @deck.title %></h3>
</div>
    <%= render partial: 'form', locals: { deck: @deck } %>
<div class="topDeck">
  <h3>Current Cards in Deck</h3>
    <div class="addedCard">
      <%= render @deck.cards %>
    </div>
</div>
<div class="topDeck">
  <h3>Add Cards To Deck</h3>
    <div class="removedCard">
      <%= render @cards %>
    </div>
</div>



